{{ define "title" }} | Login{{ end }}
{{ define "content" -}}
		<div class="login-content">
			<h1 class="h1-login">Login</h1>
			<form action="POST">
				<block class="labels">Username:<br></block>
				<input type="text" name="username" min="2" max="20"><br>
				<block class="labels">Password:<br></block>
				<input type="password" name="password" min="6" max="25"><br>
				<div class="login-button">
				<br>
				<input type="submit" value="Log In">
				<br>
				</div>
			</form> 
		</div>
{{- end }}
