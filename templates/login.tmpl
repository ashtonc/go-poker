{{ define "title" }} | Login{{ end }}
{{ define "content" -}}
		<h1>Login</h1>
		<form method="POST">
			<div>
				<label for="username">Username:</label>
				<input id="username" type="text">
			</div>
			<div>
				<label for="password">Password:</label>
				<input id="password" type="password">
			</div>
			<div>
				<input type="submit" value="Login">
			</div>
		</form>
{{- end }}
